{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=your_database_name;Username=your_username;Password=your_password;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Mailserver": {
    "Hostname": "your-smtp-server.com",
    "Port": 587,
    "Username": "your_email_username",
    "Password": "your_email_password",
    "EnableSsl": true
  },
  "JwtSettings": {
    "Secret": "your_super_secret_jwt_key_min_32_characters_long",
    "Issuer": "FoodDiaryApi",
    "Audience": "FoodDiaryApiUsers",
    "ExpiresInMinutes": 60
  },
  "FrontendUrl": "http://localhost:4173"
}
